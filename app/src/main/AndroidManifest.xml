<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.holographicplatformapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/icon_logo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_9"
        android:roundIcon="@mipmap/icon_logo"
        android:supportsRtl="true"
        android:theme="@style/Base.AppTheme">
        <activity android:name=".activity.dtjcx.SearchActivity"></activity>
        <activity android:name=".activity.cx.QueryDetailsActivity" />
        <activity android:name=".activity.tj.CountDetailsActivity" />
        <activity android:name=".activity.tj.StatisticsActivity" />
        <activity android:name=".activity.zyfl.ResourceDetailsActivity" />
        <activity android:name=".activity.zyfl.ResourceTitleActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>